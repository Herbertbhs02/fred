<%- include("partials/header"); -%>
<div class="push-footer center">
<h4>Contact Us</h4>   

<div class="row container">
  <!-- <form  action="/ContactUs" method="post" class="col s12"> -->
    <form  action="" method="" class="col s12">
      <div class="row">
        <div class="input-field col s6">
          <input id="first_name" type="text" name="firstName" class="validate">
          <label for="first_name">First Name</label>
        </div>
        <div class="input-field col s6">
          <input id="last_name" type="text" name="lastName" class="validate">
          <label for="last_name">Last Name</label>
        </div>
      </div>
    
      <div class="row">
        <div class="input-field col s12">
          <input id="email" type="email" name="email" class="validate">
          <label for="email">Email</label>
        </div>
      </div>

      <div class="row">
        <div class="input-field col s12">
          <textarea id="textarea1" name="shortMessage" class="materialize-textarea"></textarea>
          <label for="textarea1">Feel free to leave a short message</label>
        </div>
      </div>

      </div>
      <button class="btn form-btn z-depth-2 grey darken-1 " type="submit" name="button">submit</button>
    </form>
  </div>


</div>
<%- include("partials/footer"); -%>